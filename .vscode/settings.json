{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "__pycache__": true,
        ".vscode": true,
        "LICENSE": true,
        "venv": true
    },
    "hide-files.files": [
        "__pycache__",
        ".vscode",
        "LICENSE",
        "venv"
    ],
    "files.eol": "\n"
}